{% block select2_javascript %}
{% spaceless %}
<script type="text/javascript">
    jQuery(document).ready(function($) {
        $field = $('#{{ id }}');

    {% block select2_javascript_prototype %}
        $field.select2({{ configs|json_encode|raw }});
    {% endblock %}
    });
</script>
{% endspaceless %}
{% endblock select2_javascript %}

